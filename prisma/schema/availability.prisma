enum WeekDay {
  MONDAY
  TUESDAY
  WEDNESDAY
  THURSDAY
  FRIDAY
  SATURDAY
  SUNDAY
}

model Availability {
  id        String @id @default(uuid())
  day       WeekDay
  startTime String
  endTime   String

  tutorId String
  tutor   TutorProfile @relation(fields: [tutorId], references: [id])

  @@map("availability")
}
